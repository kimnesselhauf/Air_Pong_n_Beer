
<div class="bg-light">
  <h1 class="listing-title">Tables</h1>
  <%= render 'search' %>
  <div class="main-container">
  <div
    id="map"
    data-markers="<%= @markers.to_json %>"
    data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
  ></div>
    <div class="card-container">
      <% @listings.each do |listing| %>
        <%= render 'card', listing: listing %>
      <% end %>
    </div>
  </div>
</div>
